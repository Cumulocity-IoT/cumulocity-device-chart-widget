{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gp-lib-device-chart": {
      "projectType": "library",
      "root": "projects/gp-lib-device-chart",
      "sourceRoot": "projects/gp-lib-device-chart/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/gp-lib-device-chart/tsconfig.lib.json",
            "project": "projects/gp-lib-device-chart/ng-package.json"
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "DeviceChart:build",
            "proxyConfig": "src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "DeviceChart:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "DeviceChart:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/gp-lib-device-chart/src/test.ts",
            "tsConfig": "projects/gp-lib-device-chart/tsconfig.spec.json",
            "karmaConfig": "projects/gp-lib-device-chart/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/gp-lib-device-chart/tsconfig.lib.json",
              "projects/gp-lib-device-chart/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "gp-lib-firmware-chart"
}